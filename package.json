{
  "name": "wdio-visual-regression",
  "version": "1.2.1",
  "description": "Visual regression tool for webdriver.io",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc && node ./scripts/postbuild.js",
    "start": "tsc -w",
    "lint": "eslint --ext .ts packages",
    "e2e": "wdio run wdio.conf.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
